{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/zlux-file-explorer",
  "assets": [
    {
      "input": "src/assets",
      "glob": "**/*.*",
      "output": "assets"
    }
  ],
  "lib": {
    "entryFile": "src/public-api.ts"
  },
  "allowedNonPeerDependencies": [
    "primeng",
    "streamsaver",
    "web-streams-polyfill"
  ]
}